    "cookie-parser": "^1.4.7",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "form-data": "^4.0.3",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "node-fetch": "^3.3.2",
    "sharp": "^0.34.2",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
